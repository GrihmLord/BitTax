{
  "name": "bittax",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "electron": "set NODE_ENV=development && electron main.js",
    "electron-pack": "electron-builder",
    "dist": "electron-builder --win nsis"
  },
  "build": {
    "appId": "com.bittax.audit",
    "productName": "BitTax Audit",
    "win": {
      "target": "nsis"
    },
    "files": [
      "main.js",
      "audit_demo.html",
      "package.json"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/native": "^6.1.9",
    "@reduxjs/toolkit": "^1.9.7",
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.1",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.5",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|bittax|react-native-button)/)"
    ],
    "setupFiles": [
      "./path-to-setup-file.js"
    ]
  },
  "private": true
}